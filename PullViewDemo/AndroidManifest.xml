<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.opensource.pullview.demo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".PullScrollViewActivity" />
        <activity android:name=".PullListViewActivity" />
        <activity android:name=".PullListView2Activity" />
        <activity android:name=".PullGridViewActivity" />
    </application>

</manifest>
